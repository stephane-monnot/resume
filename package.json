  "scripts": {
    "fix-normalize-css-unit": "cross-env NODE_ENV=production babel --presets es2015 ./node_modules/normalize-css-unit/dist --out-dir ./node_modules/normalize-css-unit/dist",
    "start": "node scripts/customized-config  start",
    "build": "npm run fix-normalize-css-unit && node scripts/customized-config build && react-snap || true",
    "local-analytics": "replace-in-file 'https://www.google-analytics.com/' '/' build/static/js/main.*.js build/**/*.html build/*.html",
    "test": "node scripts/customized-config test --env=jsdom",
    "lint": "npm run lint:eslint -- .",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "eject": "node scripts/customized-config eject"
  },
  "lint-staged": {
    "*.js": "yarn lint:eslint"
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.0",
    "react-snap": "~1.10.0",
    "replace-in-file": "^3.1.0",
    "svg-inline-loader": "^0.8.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
