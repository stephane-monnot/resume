  "scripts": {
    "fix-normalize-css-unit": "cross-env NODE_ENV=production babel --presets es2015 ./node_modules/normalize-css-unit/dist --out-dir ./node_modules/normalize-css-unit/dist",
    "start": "node scripts/customized-config  start",
    "build": "npm run fix-normalize-css-unit && node scripts/customized-config build && react-snap || true",
    "local-analytics": "replace-in-file 'https://www.google-analytics.com/' '/' build/static/js/main.*.js build/**/*.html build/*.html",
    "test": "node scripts/customized-config test --env=jsdom",
    "eject": "node scripts/customized-config eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "react-snap": "^1.1.1",
    "replace-in-file": "^3.1.0",
    "svg-inline-loader": "^0.8.0"
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
